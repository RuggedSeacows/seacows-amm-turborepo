{
  "name": "sercows-amm-server",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Holegots",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yolominds/seacows-amm-server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "prisma generate && nest build ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "pnpm dlx prisma migrate deploy --preview-feature",
    "migrate:status": "pnpm dlx prisma migrate status --preview-feature",
    "migrate:resolve": "pnpm dlx prisma migrate resolve --preview-feature",
    "prisma:studio": "pnpm dlx prisma studio",
    "prisma:generate": "pnpm dlx prisma generate",
    "prisma:generate:watch": "pnpm dlx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "pnpm run migrate:up && pnpm run prisma:generate && pnpm run seed",
    "seed": "prisma db seed",
    "postinstall": "pnpm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api pnpm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "2.2.2",
    "@nestjs/apollo": "10.0.19",
    "@nestjs/common": "9.0.5",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.5",
    "@nestjs/graphql": "10.0.20",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "6.0.4",
    "@prisma/client": "4.1.0",
    "@seacows/amm-abi": "workspace:^1.0.0",
    "@supercharge/promise-pool": "^2.3.2",
    "apollo-server-express": "3.10.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "ethers": "^5.7.1",
    "graphql": "16.5.0",
    "graphql-scalars": "1.17.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "nest-web3": "^1.1.3",
    "nestjs-http-promise": "^2.0.0",
    "nestjs-prisma": "0.17.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "prisma-nestjs-graphql": "^17.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "web3": "^1.7.5",
    "web3-core": "^1.7.5",
    "web3-eth-contract": "^1.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.5",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/cron": "^2.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "^18.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "chance": "1.1.8",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "prisma": "4.1.0",
    "prisma-dbml-generator": "0.10.0-dev.0",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-morph": "^15.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/prisma/generated/prisma-client"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src",
      "<rootDir>/prisma/generated/prisma-client"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
